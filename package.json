{
  "version": "0.0.1",
  "main": "dist/server.js",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "browser-express": "^3.2.0",
    "browserify": "^14.4.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "enzyme": "^2.9.1",
    "eslint-watch": "^3.1.2",
    "execa": "^0.7.0",
    "express": "^4.15.3",
    "express-browserify": "^1.0.2",
    "import-export": "^1.0.1",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "jsonfile": "^3.0.1",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.0",
    "lodash.reverse": "^4.0.1",
    "lodash.set": "^4.3.2",
    "moment": "^2.18.1",
    "nodemon": "^1.11.0",
    "npm-install-global": "^1.0.0",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "regenerator-runtime": "^0.11.0",
    "require-dir": "^0.3.2",
    "rethinkdb": "^2.3.3",
    "scssify": "^2.3.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "socket.io-frontend": "^0.0.2",
    "unzip": "^0.1.11",
    "yarn-api": "^1.1.0"
  },
  "babel": {
    "minified": true,
    "presets": [
      "env",
      "stage-0",
      "react"
    ],
    "plugins": [
      [
        "transform-es2015-modules-commonjs"
      ],
      [
        "transform-runtime"
      ]
    ],
    "ignore": [
      "dist/**/*",
      "node_modules/**/*",
      "nobs.js"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ],
      [
        "scssify",
        {
          "_flags": {
            "debug": true
          },
          "autoInject": true
        }
      ]
    ]
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint:watch": "esw -w src/**/*.js",
    "babel": "babel src --out-dir dist/ --copy-files",
    "browserify": "browserify dist/client.js -o dist/public/client.js",
    "build": "npm-run-all lint babel browserify",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "nodemon": "cross-env NODE_ENV=dev nodemon",
    "dev": "concurrently 'npm run -s test:watch' 'npm run -s lint:watch' 'npm run -s nodemon'"
  },
  "nodemonConfig": {
    "ignore": [
      "*test.js"
    ],
    "ext": "js,jsx,html,json,css,scss",
    "watch": "src",
    "exec": "babel-node index.js"
  },
  "bugs": {
    "url": "https://github.com/abraham-serafino/nobs/issues"
  },
  "bin": {
    "nobs": "nobs.js"
  },
  "directories": {
    "lib": "lib"
  },
  "name": "No.BS",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      ".*": "jest-css-modules",
      "^*$": "babel-jest"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.7.2",
    "eslint-plugin-react": "7.3.0"
  }
}
